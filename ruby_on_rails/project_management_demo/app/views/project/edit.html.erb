<h1>Edit Project Detail</h1>

<%= form_for @project, :url =>{:action => "update", :id =>@project} do |f| %>
  <div class="form-group">
      <p><label for = "project_title">Title</label>: <%= f.text_field 'title' %></p>
  </div>
  <div class="form-group">
      <p><label for = "project_teamlead_id">LeamLead</label>: <%= f.collection_select :teamlead_id, Teamlead.all, :id, :name %></p>
  </div>
  <div class="form-group">
      <label for = "project_description">Description</label>: <%= f.text_area 'description' %></p>
  </div>
  <hr />
  <%= f.submit "Update", {:class => 'btn btn-xm btn-primary'} %>
  <%= link_to 'Back', {:action => 'list' }, {:class => 'btn btn-xm btn-primary'}%>
<% end %>
</div>